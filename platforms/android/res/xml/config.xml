<?xml version='1.0' encoding='utf-8'?>
<widget id="com.unilunch.app" version="1.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="GoogleMaps">
        <param name="android-package" value="plugin.google.maps.GoogleMaps" />
    </feature>
    <feature name="External">
        <param name="android-package" value="plugin.google.maps.External" />
    </feature>
    <feature name="Geocoder">
        <param name="android-package" value="plugin.google.maps.MyGeocoder" />
    </feature>
    <name>unimenu</name>
    <description>
        An application which shows the menus of different student priced restaurants
    </description>
    <author email="play-store@huotari.me" href="http://huotari.me">
        Jonne Huotari
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="backgroundColor" value="0xffffffff" />
    <preference name="KeepRunning" value="false" />
</widget>

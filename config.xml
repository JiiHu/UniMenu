<?xml version='1.0' encoding='utf-8'?>
<widget id="com.unilunch.app" version="1.0.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>unimenu</name>
    <description>
        An application which shows the menus of different student priced restaurants
    </description>
    <author email="play-store@huotari.me" href="http://huotari.me">
        Jonne Huotari
    </author>
    <content src="index.html" />
    <allow-navigation href="*" />
    <allow-intent href="*" />
    <access origin="*" />
    <preference name="backgroundColor" value="0xffffffff" />
    <preference name="KeepRunning" value="false" />
    <plugin name="NetworkStatus" value="org.apache.cordova.NetworkManager" />
    <icon src="icon.png" />
    <engine name="ios" spec="~4.0.1" />
</widget>
